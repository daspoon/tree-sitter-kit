{
  "name": "ExprLang",
  "rules": {
    "start": {"type": "SYMBOL", "name": "_Expr"},
    "_Expr": {"type": "CHOICE", "members": [{"type": "SYMBOL", "name": "Expr_mul"}, {"type": "SYMBOL", "name": "Expr_pow"}, {"type": "SYMBOL", "name": "Expr_name"}, {"type": "SYMBOL", "name": "Expr_neg"}, {"type": "SYMBOL", "name": "Expr_apply"}, {"type": "SYMBOL", "name": "Expr_value"}, {"type": "SYMBOL", "name": "Expr_add"}, {"type": "SYMBOL", "name": "Expr_paren"}]},
    "Expr_mul": {"type": "PREC_LEFT", "value": 2, "content": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "FIELD", "name": "1", "content": {"type": "CHOICE", "members": [{"type": "STRING", "value": "*"}, {"type": "STRING", "value": "/"}]}}, {"type": "FIELD", "name": "2", "content": {"type": "SYMBOL", "name": "_Expr"}}]}},
    "Expr_pow": {"type": "PREC_LEFT", "value": 3, "content": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "FIELD", "name": "1", "content": {"type": "CHOICE", "members": [{"type": "STRING", "value": "^"}]}}, {"type": "FIELD", "name": "2", "content": {"type": "SYMBOL", "name": "_Expr"}}]}},
    "Expr_name": {"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "Name"}},
    "Expr_neg": {"type": "PREC_LEFT", "value": 4, "content": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "CHOICE", "members": [{"type": "STRING", "value": "-"}]}}, {"type": "FIELD", "name": "1", "content": {"type": "SYMBOL", "name": "_Expr"}}]}},
    "Expr_apply": {"type": "PREC", "value": 5, "content": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "STRING", "value": "("}, {"type": "CHOICE", "members": [{"type": "FIELD", "name": "1", "content": {"type": "SYMBOL", "name": "ExprList"}}, {"type": "BLANK"}]}, {"type": "STRING", "value": ")"}]}},
    "Expr_value": {"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "Value"}},
    "Expr_add": {"type": "PREC_LEFT", "value": 1, "content": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "FIELD", "name": "1", "content": {"type": "CHOICE", "members": [{"type": "STRING", "value": "+"}, {"type": "STRING", "value": "-"}]}}, {"type": "FIELD", "name": "2", "content": {"type": "SYMBOL", "name": "_Expr"}}]}},
    "Expr_paren": {"type": "SEQ", "members": [{"type": "STRING", "value": "("}, {"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "STRING", "value": ")"}]},
    "ExprList": {"type": "SEQ", "members": [{"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}, {"type": "REPEAT", "content": {"type": "SEQ", "members": [{"type": "STRING", "value": ","}, {"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "_Expr"}}]}}]},
    "Name": {"type": "FIELD", "name": "0", "content": {"type": "PATTERN", "value": "[a-zA-Z_][0-9a-zA-Z_]*"}},
    "Value": {"type": "FIELD", "name": "0", "content": {"type": "SYMBOL", "name": "Int"}},
    "Int": {"type": "FIELD", "name": "0", "content": {"type": "PATTERN", "value": "[0-9]+"}},
    "Word": {"type": "PATTERN", "value": "([a-zA-Z_][0-9a-zA-Z_]* | [!#%&*+-/<=>^|~]+)"}
  },
  "word": "Word",
  "extras": [{"type": "PATTERN", "value": "\\s"}],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
